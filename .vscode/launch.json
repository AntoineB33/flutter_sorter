{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run debug.js",
      "program": "${workspaceFolder}/assets/js/debug.js"
    },
    {
      "name": "Flutter (Chrome, Verbose)",
      "request": "launch",
      "type": "dart",
      "deviceId": "chrome",
      "program": "lib/main.dart",
      "args": [
        "--verbose",
        "--start-paused",
        "--release"
      ]
    },
    {
      "name": "Flutter (Windows, Verbose)",
      "request": "launch",
      "type": "dart",
      "deviceId": "windows",
      "program": "lib/main.dart",
      "args": [
        "--verbose",
        "--start-paused"
      ]
    }
  ]
}
